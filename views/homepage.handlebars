{{#each blogs}}
<div class="row mb-4 blog">
    <div class="col-sm-5">
        <h2>
            <p><a href="/blog/{{id}}">{{title}}</a>
            Posted by {{User.name}} on {{format_date date_created}}</p>
        </h2>
    </div>
    <div class="data">
    <p1>
        {{blog.data}}
    </p1>
    </div>
</div>
{{/each}}

